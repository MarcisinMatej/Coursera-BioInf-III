    "def read_seq_file(path):\n",
    "    \"\"\"\n",
    "    Reads file which contains 2 sequences separated by new line.\n",
    "    \"\"\"\n",
    "    with open(path) as f:\n",
    "        seq1 = f.readline().strip()\n",
    "        seq2 = f.readline().strip()\n",
    "    return seq1, seq2"
